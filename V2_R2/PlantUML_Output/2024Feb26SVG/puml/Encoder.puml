	eInnerState_SetIncEncoderHome_Node_0 ---> eInnerState_SetIncEncoderHome_Node_3 : (NOT (bHomePositionOverride))
	eInnerState_SetIncEncoderHome_Node_3 ---> eInnerState_SetIncEncoderHome_Node_4 : (fbHome.Done)
