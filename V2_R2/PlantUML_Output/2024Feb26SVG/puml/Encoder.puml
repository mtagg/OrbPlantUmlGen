	eInnerState_SetIncEncoderHome_Node_5 ---> eInnerState_SetIncEncoderHome_Node_6 : (fbHome.Error)
	eInnerState_SetIncEncoderHome_Node_6 ---> eInnerState_SetIncEncoderHome_ERROR : (fbHome.Error)
